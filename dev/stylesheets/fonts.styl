@font-face {
        font-family: 'mgl';
        src: url('../f/Mission Gothic Light_web.eot');
        src: url('../f/Mission Gothic Light_web.eot?#iefix') format('embedded-opentype'),
             url('../f/Mission Gothic Light_web.woff') format('woff');
        font-weight: normal;
        font-style: normal;
    }

@font-face {
        font-family: 'mgb';
        src: url('../f/Mission Gothic Black_web.eot');
        src: url('../f/Mission Gothic Black_web.eot?#iefix') format('embedded-opentype'),
             url('../f/Mission Gothic Black_web.woff') format('woff');
        font-weight: bold;
        font-style: normal;
    }